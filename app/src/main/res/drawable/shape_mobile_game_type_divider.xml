<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="rectangle">

            <stroke
                android:width="2px"
                android:color="@color/line_grey" />
            <size android:height="12px" />
        </shape>
    </item>
    <item
        android:bottom="1px"
        android:top="1px">
        <shape
            android:dither="true"
            android:shape="rectangle">
            <solid android:color="@color/bg_grey" />
            <size android:height="12px" />
        </shape>
    </item>

</layer-list>